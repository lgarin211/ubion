# Production Environment Configuration
# Copy this to .env.local for production deployment

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://your-production-api.com
NEXT_PUBLIC_API_TIMEOUT=10000

# Session/Cache Configuration (in minutes)
NEXT_PUBLIC_CACHE_TIMEOUT=30
NEXT_PUBLIC_SESSION_TIMEOUT=60

# Web Status Configuration - IMPORTANT FOR PRODUCTION
# Set to 'prod' to hide all debug information and API details
NEXT_PUBLIC_WEB_STATUS=prod
